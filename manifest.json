{
	"manifest_version": 2,
	"name": "Instagram - No Login Required",
	"version": "0.1",
	"description": "View Instagram posts without logging in.",
	"icons": {
      "16": "images/icon16x16.png",
      "32": "images/icon32x32.png",
      "48": "images/icon48x48.png",
      "64": "images/icon64x64.png",
      "128": "images/icon128x128.png"
    },
	"content_scripts": [
	{
		"matches": [
		"https://www.instagram.com/*"
		],
		"js": ["jquery-3.5.1.min.js", "showImage.js"]
		}
	]
}